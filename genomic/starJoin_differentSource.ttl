@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .
@base <http://tib.de/SDM-RDFizer/mapping#> .

<TriplesMap1>
	rml:logicalSource [ rml:source "/home/mevidal/www/cikm2020/starJoin/source1.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Mutation/{MUTATION_ID}";
	];
	rr:predicateObjectMap [
		rr:predicate iasis:mutation_isRelatedTo_gene;
		rr:objectMap [
			rr:parentTriplesMap <TriplesMap2>;
			rr:joinCondition [
				rr:child "MUTATION_ID";
				rr:parent "MUTATION_ID";
			];
		];
	];	
	rr:predicateObjectMap [
		rr:predicate iasis:mutation_isRelatedTo_sample;
		rr:objectMap [
			rr:parentTriplesMap <TriplesMap3>;
			rr:joinCondition [
				rr:child "MUTATION_ID";
				rr:parent "MUTATION_ID";
			];
		];
	];
	rr:predicateObjectMap [
		rr:predicate iasis:mutation_isObservedIn_GenomePosition;
		rr:objectMap [
			rr:parentTriplesMap <TriplesMap4>;
			rr:joinCondition [
				rr:child "MUTATION_ID";
				rr:parent "MUTATION_ID";
			];
		];
	];
	rr:predicateObjectMap [
		rr:predicate iasis:mutation_isObservedIn_tumor;
		rr:objectMap [
			rr:parentTriplesMap <TriplesMap5>;
			rr:joinCondition [
				rr:child "MUTATION_ID";
				rr:parent "MUTATION_ID";
			];
		];
	];
	rr:predicateObjectMap [
		rr:predicate iasis:has_somatic_status;
		rr:objectMap [
			rr:parentTriplesMap <TriplesMap6>;
			rr:joinCondition [
				rr:child "MUTATION_ID";
				rr:parent "MUTATION_ID";
			];
		];
	].

<TriplesMap2>	
	rml:logicalSource [ rml:source "/home/mevidal/www/cikm2020/starJoin/source2.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Gene/{Gene name}";
	];
	rr:predicateObjectMap [
		rr:predicate rdf:type;
		rr:objectMap [
			rr:constant "http://project-iasis.eu/vocab/Gene";
		];
	].

